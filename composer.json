{
	"name": "axllent/silverstripe-analytics-js",
	"description": "Google Universal Analytics.js tracking code for SilverStripe",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/axllent/silverstripe-analytics-js",
	"keywords": ["silverstripe", "analytics", "googleanalytics", "tracking"],
	"license": "The MIT License (MIT)",
	"authors": [
		{
			"name": "Ralph Slooten",
			"homepage": "http://www.axllent.org/"
		}
	],
	"support": {
		"issues": "https://github.com/axllent/silverstripe-analytics-js/issues"
	},
	"require": {
		"silverstripe/framework": "^4.0"
	},
    "suggest": {
        "guzzlehttp/guzzle": "optional: if you require caching of analytics.js"
    },
    "autoload": {
        "psr-4": {
            "Axllent\\AnalyticsJS\\": "src/"
        }
    }
}
